		GNET::Protocol *Clone() const {  return new EC_SQLSetPlayerRanking(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_EC_SQLSETPLAYERRANKING };
		EC_SQLSetPlayerRanking(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
