	public:
		int idx;
		int roleid;
		int kickout_id;
		int team_type;
		enum { PROTOCOL_TYPE = PROTOCOL_EC_ARENATEAMKICKOUT };
	public:
		EC_ArenaTeamKickout() { type = PROTOCOL_EC_ARENATEAMKICKOUT; }
		EC_ArenaTeamKickout(void*) : Protocol(PROTOCOL_EC_ARENATEAMKICKOUT) { }
		EC_ArenaTeamKickout (int l_idx = 0,int l_roleid = 0,int l_kickout_id = 0,
			int l_team_type = 0)
			 : idx(l_idx),roleid(l_roleid),kickout_id(l_kickout_id)
			,team_type(l_team_type)
		{
			type = PROTOCOL_EC_ARENATEAMKICKOUT;
		}

		EC_ArenaTeamKickout(const EC_ArenaTeamKickout &rhs)
			: Protocol(rhs),idx(rhs.idx),roleid(rhs.roleid),kickout_id(rhs.kickout_id)
			,team_type(rhs.team_type) { }

		GNET::Protocol *Clone() const { return new EC_ArenaTeamKickout(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << idx;
			os << roleid;
			os << kickout_id;
			os << team_type;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> idx;
			os >> roleid;
			os >> kickout_id;
			os >> team_type;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 32; }
