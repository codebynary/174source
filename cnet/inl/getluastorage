		GNET::Protocol *Clone() const {  return new GetLuaStorage(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_GETLUASTORAGE };
		GetLuaStorage(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		GetLuaStorage(const GetLuaStorage &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 8192; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
