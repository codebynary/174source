	public:
		int retcode;
		int roleid;
		EC_ArenaTeamVector teams;
		EC_ArenaPlayerVector members;
		enum { PROTOCOL_TYPE = PROTOCOL_EC_ARENAPLAYERTOTALINFOQUERY_RE };
	public:
		EC_ArenaPlayerTotalInfoQuery_Re() { type = PROTOCOL_EC_ARENAPLAYERTOTALINFOQUERY_RE; }
		EC_ArenaPlayerTotalInfoQuery_Re(void*) : Protocol(PROTOCOL_EC_ARENAPLAYERTOTALINFOQUERY_RE) { }
		EC_ArenaPlayerTotalInfoQuery_Re (int l_retcode = 0,int l_roleid = 0,const EC_ArenaTeamVector& l_teams = EC_ArenaTeamVector(),
			const EC_ArenaPlayerVector& l_members = EC_ArenaPlayerVector())
			 : retcode(l_retcode),roleid(l_roleid),teams(l_teams)
			,members(l_members)
		{
			type = PROTOCOL_EC_ARENAPLAYERTOTALINFOQUERY_RE;
		}

		EC_ArenaPlayerTotalInfoQuery_Re(const EC_ArenaPlayerTotalInfoQuery_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),teams(rhs.teams)
			,members(rhs.members) { }

		GNET::Protocol *Clone() const { return new EC_ArenaPlayerTotalInfoQuery_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << teams;
			os << members;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> teams;
			os >> members;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 31744; }
