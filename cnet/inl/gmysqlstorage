		GNET::Protocol *Clone() const {  return new GMySQLStorage(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_GMYSQLSTORAGE };
		GMySQLStorage(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		GMySQLStorage(const GMySQLStorage &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 65535; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
