	public:
		int retcode;
		int roleid;
		EC_ArenaTeamSyncVector teams;
		EC_ArenaPlayerSyncVector members;
		enum { PROTOCOL_TYPE = PROTOCOL_EC_ARENAQUERY_RE };
	public:
		EC_ArenaQuery_Re() { type = PROTOCOL_EC_ARENAQUERY_RE; }
		EC_ArenaQuery_Re(void*) : Protocol(PROTOCOL_EC_ARENAQUERY_RE) { }
		EC_ArenaQuery_Re (int l_retcode = 0,int l_roleid = 0,const EC_ArenaTeamSyncVector& l_teams = EC_ArenaTeamSyncVector(),
			const EC_ArenaPlayerSyncVector& l_members = EC_ArenaPlayerSyncVector())
			 : retcode(l_retcode),roleid(l_roleid),teams(l_teams)
			,members(l_members)
		{
			type = PROTOCOL_EC_ARENAQUERY_RE;
		}

		EC_ArenaQuery_Re(const EC_ArenaQuery_Re &rhs)
			: Protocol(rhs),retcode(rhs.retcode),roleid(rhs.roleid),teams(rhs.teams)
			,members(rhs.members) { }

		GNET::Protocol *Clone() const { return new EC_ArenaQuery_Re(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << roleid;
			os << teams;
			os << members;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> roleid;
			os >> teams;
			os >> members;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 10240; }
