		GNET::Protocol *Clone() const {  return new SetNewRoleDetail(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_SETNEWROLEDETAIL };
		SetNewRoleDetail(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		SetNewRoleDetail(const SetNewRoleDetail &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 65536; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
