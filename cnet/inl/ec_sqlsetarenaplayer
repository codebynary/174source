		GNET::Protocol *Clone() const {  return new EC_SQLSetArenaPlayer(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_EC_SQLSETARENAPLAYER };
		EC_SQLSetArenaPlayer(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		EC_SQLSetArenaPlayer(const EC_SQLSetArenaPlayer &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 31744; }
		bool TimePolicy(int timeout) const { return timeout <= 120; }
