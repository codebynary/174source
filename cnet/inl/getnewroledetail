		GNET::Protocol *Clone() const {  return new GetNewRoleDetail(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_GETNEWROLEDETAIL };
		GetNewRoleDetail(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		GetNewRoleDetail(const GetNewRoleDetail &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 65536; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
