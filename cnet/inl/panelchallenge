	public:
		Octets nonce;
		enum { PROTOCOL_TYPE = PROTOCOL_PANELCHALLENGE };
	public:
		PanelChallenge() { type = PROTOCOL_PANELCHALLENGE; }
		PanelChallenge(void*) : Protocol(PROTOCOL_PANELCHALLENGE) { }
		PanelChallenge (const Octets& l_nonce = Octets())
			 : nonce(l_nonce)
		{
			type = PROTOCOL_PANELCHALLENGE;
		}

		PanelChallenge(const PanelChallenge &rhs)
			: Protocol(rhs),nonce(rhs.nonce) { }

		GNET::Protocol *Clone() const { return new PanelChallenge(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << nonce;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> nonce;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 64; }
