		GNET::Protocol *Clone() const {  return new GetNewTrashBox(*this); }
	public:
		enum { PROTOCOL_TYPE = RPC_GETNEWTRASHBOX };
		GetNewTrashBox(Type type, Rpc::Data *argument, Rpc::Data *result)
			: RPC_BASECLASS(type, argument, result ) { }
		GetNewTrashBox(const GetNewTrashBox &rhs) : RPC_BASECLASS(rhs) { }
		int  PriorPolicy( ) const { return 1; }
		bool SizePolicy(size_t size) const { return size <= 4096; }
		bool TimePolicy(int timeout) const { return timeout <= 20; }
