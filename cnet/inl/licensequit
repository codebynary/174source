	public:
		int id;
		unsigned int success;
		enum { PROTOCOL_TYPE = PROTOCOL_LICENSEQUIT };
	public:
		LicenseQuit() { type = PROTOCOL_LICENSEQUIT; }
		LicenseQuit(void*) : Protocol(PROTOCOL_LICENSEQUIT) { }
		LicenseQuit (int l_id = 0,unsigned int l_success = 0)
			 : id(l_id),success(l_success)
		{
			type = PROTOCOL_LICENSEQUIT;
		}

		LicenseQuit(const LicenseQuit &rhs)
			: Protocol(rhs),id(rhs.id),success(rhs.success) { }

		GNET::Protocol *Clone() const { return new LicenseQuit(*this); }

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << id;
			os << success;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> id;
			os >> success;
			return os;
		}

		int PriorPolicy( ) const { return 1; }

		bool SizePolicy(size_t size) const { return size <= 16; }
