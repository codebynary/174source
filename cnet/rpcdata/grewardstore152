#ifndef __GNET_GREWARDSTORE152_RPCDATA
#define __GNET_GREWARDSTORE152_RPCDATA

#include "rpcdefs.h"

#include "grewarditem"

namespace GNET
{
	class GRewardStore152 : public GNET::Rpc::Data
	{
	public:
		int consume_points;
		int bonus_reward;
		GRewardItemVector rewardlist;
		int reserved1;
		int reserved2;
		int reserved3;
		int reserved4;

	public:
		GRewardStore152 (int l_consume_points = 0,int l_bonus_reward = 0,int l_reserved1 = 0
			,int l_reserved2 = 0,int l_reserved3 = 0,int l_reserved4 = 0)
			: consume_points(l_consume_points),bonus_reward(l_bonus_reward),reserved1(l_reserved1)
			,reserved2(l_reserved2),reserved3(l_reserved3),reserved4(l_reserved4)
		{
		}

		GRewardStore152(const GRewardStore152 &rhs)
			: consume_points(rhs.consume_points),bonus_reward(rhs.bonus_reward),rewardlist(rhs.rewardlist),
			reserved1(rhs.reserved1),reserved2(rhs.reserved2),reserved3(rhs.reserved3),
			reserved4(rhs.reserved4) { }

		Rpc::Data *Clone() const { return new GRewardStore152(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const GRewardStore152 *r = dynamic_cast<const GRewardStore152 *>(&rhs);
			if (r && r != this)
			{
				consume_points = r->consume_points;
				bonus_reward = r->bonus_reward;
				rewardlist = r->rewardlist;
				reserved1 = r->reserved1;
				reserved2 = r->reserved2;
				reserved3 = r->reserved3;
				reserved4 = r->reserved4;
			}
			return *this;
		}

		GRewardStore152& operator = (const GRewardStore152 &rhs)
		{
			const GRewardStore152 *r = &rhs;
			if (r && r != this)
			{
				consume_points = r->consume_points;
				bonus_reward = r->bonus_reward;
				rewardlist = r->rewardlist;
				reserved1 = r->reserved1;
				reserved2 = r->reserved2;
				reserved3 = r->reserved3;
				reserved4 = r->reserved4;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << consume_points;
			os << bonus_reward;
			os << rewardlist;
			os << reserved1;
			os << reserved2;
			os << reserved3;
			os << reserved4;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> consume_points;
			os >> bonus_reward;
			os >> rewardlist;
			os >> reserved1;
			os >> reserved2;
			os >> reserved3;
			os >> reserved4;
			return os;
		}

	};
};
#endif
