#ifndef __GNET_EC_SQLGETPLAYERRANKINGRES_RPCDATA
#define __GNET_EC_SQLGETPLAYERRANKINGRES_RPCDATA

#include "rpcdefs.h"

#include "ec_sqlplayerranking"

namespace GNET
{
	class EC_SQLGetPlayerRankingRes : public GNET::Rpc::Data
	{
	public:
		int retcode;
		EC_SQLPlayerRanking player;

	public:
		EC_SQLGetPlayerRankingRes (int l_retcode = -1,const EC_SQLPlayerRanking& l_player = EC_SQLPlayerRanking())
			: retcode(l_retcode),player(l_player)
		{
		}

		EC_SQLGetPlayerRankingRes(const EC_SQLGetPlayerRankingRes &rhs)
			: retcode(rhs.retcode),player(rhs.player) { }

		Rpc::Data *Clone() const { return new EC_SQLGetPlayerRankingRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const EC_SQLGetPlayerRankingRes *r = dynamic_cast<const EC_SQLGetPlayerRankingRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
				player = r->player;
			}
			return *this;
		}

		EC_SQLGetPlayerRankingRes& operator = (const EC_SQLGetPlayerRankingRes &rhs)
		{
			const EC_SQLGetPlayerRankingRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
				player = r->player;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			os << player;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			os >> player;
			return os;
		}

	};
};
#endif
