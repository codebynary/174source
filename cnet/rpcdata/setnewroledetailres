#ifndef __GNET_SETNEWROLEDETAILRES_RPCDATA
#define __GNET_SETNEWROLEDETAILRES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class SetNewRoleDetailRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		SetNewRoleDetailRes (int l_retcode = -1)
			: retcode(l_retcode)
		{
		}

		SetNewRoleDetailRes(const SetNewRoleDetailRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new SetNewRoleDetailRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const SetNewRoleDetailRes *r = dynamic_cast<const SetNewRoleDetailRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		SetNewRoleDetailRes& operator = (const SetNewRoleDetailRes &rhs)
		{
			const SetNewRoleDetailRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
