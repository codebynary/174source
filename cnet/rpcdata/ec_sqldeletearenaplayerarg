#ifndef __GNET_EC_SQLDELETEARENAPLAYERARG_RPCDATA
#define __GNET_EC_SQLDELETEARENAPLAYERARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class EC_SQLDeleteArenaPlayerArg : public GNET::Rpc::Data
	{
	public:
		int roleid;

	public:
		EC_SQLDeleteArenaPlayerArg (int l_roleid = 0)
			: roleid(l_roleid)
		{
		}

		EC_SQLDeleteArenaPlayerArg(const EC_SQLDeleteArenaPlayerArg &rhs)
			: roleid(rhs.roleid) { }

		Rpc::Data *Clone() const { return new EC_SQLDeleteArenaPlayerArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const EC_SQLDeleteArenaPlayerArg *r = dynamic_cast<const EC_SQLDeleteArenaPlayerArg *>(&rhs);
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		EC_SQLDeleteArenaPlayerArg& operator = (const EC_SQLDeleteArenaPlayerArg &rhs)
		{
			const EC_SQLDeleteArenaPlayerArg *r = &rhs;
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			return os;
		}

	};
};
#endif
