#ifndef __GNET_SETLUASTORAGERES_RPCDATA
#define __GNET_SETLUASTORAGERES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class SetLuaStorageRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
		SetLuaStorageRes (int l_retcode = 0)
			: retcode(l_retcode)
		{
		}

		SetLuaStorageRes(const SetLuaStorageRes &rhs)
			: retcode(rhs.retcode) { }

		Rpc::Data *Clone() const { return new SetLuaStorageRes(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const SetLuaStorageRes *r = dynamic_cast<const SetLuaStorageRes *>(&rhs);
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		SetLuaStorageRes& operator = (const SetLuaStorageRes &rhs)
		{
			const SetLuaStorageRes *r = &rhs;
			if (r && r != this)
			{
				retcode = r->retcode;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << retcode;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> retcode;
			return os;
		}

	};
};
#endif
