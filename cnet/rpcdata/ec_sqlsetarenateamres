#ifndef __GNET_EC_SQLSETARENATEAMRES_RPCDATA
#define __GNET_EC_SQLSETARENATEAMRES_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class EC_SQLSetArenaTeamRes : public GNET::Rpc::Data
	{
	public:
		int retcode;

	public:
