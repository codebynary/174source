#ifndef __GNET_EC_ARENATEAMCAPTAINTRANSFERSUCCESSDATA_RPCDATA
#define __GNET_EC_ARENATEAMCAPTAINTRANSFERSUCCESSDATA_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class EC_ArenaTeamCaptainTransferSuccessData : public GNET::Rpc::Data
	{
	public:
		int roleid;
		int transfer_id;

	public:
		EC_ArenaTeamCaptainTransferSuccessData (int l_roleid = 0,int l_transfer_id = 0)
			: roleid(l_roleid),transfer_id(l_transfer_id)
		{
		}

		EC_ArenaTeamCaptainTransferSuccessData(const EC_ArenaTeamCaptainTransferSuccessData &rhs)
			: roleid(rhs.roleid),transfer_id(rhs.transfer_id) { }

		Rpc::Data *Clone() const { return new EC_ArenaTeamCaptainTransferSuccessData(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const EC_ArenaTeamCaptainTransferSuccessData *r = dynamic_cast<const EC_ArenaTeamCaptainTransferSuccessData *>(&rhs);
			if (r && r != this)
			{
				roleid = r->roleid;
				transfer_id = r->transfer_id;
			}
			return *this;
		}

		EC_ArenaTeamCaptainTransferSuccessData& operator = (const EC_ArenaTeamCaptainTransferSuccessData &rhs)
		{
			const EC_ArenaTeamCaptainTransferSuccessData *r = &rhs;
			if (r && r != this)
			{
				roleid = r->roleid;
				transfer_id = r->transfer_id;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			os << transfer_id;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			os >> transfer_id;
			return os;
		}

	};
	typedef GNET::RpcDataVector<EC_ArenaTeamCaptainTransferSuccessData>	EC_ArenaTeamCaptainTransferSuccessDataVector;
