#ifndef __GNET_EC_SQLGETPLAYERRANKINGARG_RPCDATA
#define __GNET_EC_SQLGETPLAYERRANKINGARG_RPCDATA

#include "rpcdefs.h"


namespace GNET
{
	class EC_SQLGetPlayerRankingArg : public GNET::Rpc::Data
	{
	public:
		int roleid;

	public:
		EC_SQLGetPlayerRankingArg (int l_roleid = 0)
			: roleid(l_roleid)
		{
		}

		EC_SQLGetPlayerRankingArg(const EC_SQLGetPlayerRankingArg &rhs)
			: roleid(rhs.roleid) { }

		Rpc::Data *Clone() const { return new EC_SQLGetPlayerRankingArg(*this); }

		Rpc::Data& operator = (const Rpc::Data &rhs)
		{
			const EC_SQLGetPlayerRankingArg *r = dynamic_cast<const EC_SQLGetPlayerRankingArg *>(&rhs);
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		EC_SQLGetPlayerRankingArg& operator = (const EC_SQLGetPlayerRankingArg &rhs)
		{
			const EC_SQLGetPlayerRankingArg *r = &rhs;
			if (r && r != this)
			{
				roleid = r->roleid;
			}
			return *this;
		}

		OctetsStream& marshal(OctetsStream & os) const
		{
			os << roleid;
			return os;
		}

		const OctetsStream& unmarshal(const OctetsStream &os)
		{
			os >> roleid;
			return os;
		}

	};
};
#endif
